{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "prettier-standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "@typescript-eslint"
  ],
  "rules": {
    "camelcase": "off",
    "array-callback-return": "off",
    "no-unused-vars": "off",
    "no-use-before-define": "off"
  }
}
